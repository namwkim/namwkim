<body ng-app="myApp">
    <div class="ui container" ng-controller="myAppCtrl">
      <h1 class="ui dividing orange center aligned icon header">
        <!-- <i class="plug icon"></i> -->
        <img class="ui image" src="images/ddg-icon.svg"/>
        <div class="content">
          Data-Driven Guides
        </div>
        <div class="sub header">Supporting Expressive Design for Information Graphics</div>
      </h1>
      <div class="ui stackable grid">
        <div class="two column row">
          <div class="column"> [Video]</div>
          <div class="column"> [Abstract]</div>
        </div>
        <div class="row">
          <div class="column"> [Authors]</div>
        </div>
        <div class="row">
          <div class="column"> [Examples]</div>
        </div>
      </div>
    </div>


    <!-- javascript -->
    <script src="https://code.jquery.com/jquery-3.1.0.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.6/angular.min.js"></script>
    <script src="https://cdn.jsdelivr.net/semantic-ui/2.2.2/semantic.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/paper.js/0.10.2/paper-core.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>

    <!-- Changing the delimeter of AngularJS to avoid conflict with Hogan-Express-->
    <script type="text/javascript">
        var app = angular.module('myApp', [], function($interpolateProvider) {
                $interpolateProvider.startSymbol('<%');
                $interpolateProvider.endSymbol('%>');
            })
            .run(function($rootScope) {
                //	website initialization
            })
        app.controller('myAppCtrl', function($scope) {

        });
    </script>

